{
  "scripts": {
    "start": "concurrently \"yarn start-client\" \"yarn start-server\"",
    "start-client": "yarn --cwd client start",
    "start-server": "yarn --cwd server start",
    "install-all": "yarn install && yarn install --cwd client && yarn install --cwd server"
  },
  "dependencies": {
    "concurrently": "^9.1.0"
  }
}
